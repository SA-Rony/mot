      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router",
      "expo-font",
      "expo-web-browser"
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {},
      "eas": {
        "projectId": "d847c68a-e488-4791-b66b-9fa5bfdc129f"
      }
    },
    "android": {
      "package": "com.rony79.mot"
    }
  }
